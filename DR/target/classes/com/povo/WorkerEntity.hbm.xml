<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.povo.WorkerEntity" table="worker" schema="dormitory">
        <id name="workerid" column="workerid"/>
        <property name="password" column="password"/>
        <property name="realname" column="realname"/>
        <property name="phone" column="phone"/>
        <property name="scope" column="scope"/>
        <set name="repairsByWorkerid" inverse="true">
            <key>
                <column name="wid" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.povo.RepairEntity"/>
        </set>
    </class>
</hibernate-mapping>