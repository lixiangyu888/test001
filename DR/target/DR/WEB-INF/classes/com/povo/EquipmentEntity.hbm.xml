<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.povo.EquipmentEntity" table="equipment" schema="dormitory">
        <id name="equipmentid" column="equipmentid"/>
        <property name="building" column="building"/>
        <property name="floor" column="floor"/>
        <property name="room" column="room"/>
        <property name="scope" column="scope"/>
        <set name="repairsByEquipmentid" inverse="true">
            <key>
                <column name="eid"/>
            </key>
            <one-to-many not-found="ignore" class="com.povo.RepairEntity" />
        </set>
    </class>
</hibernate-mapping>